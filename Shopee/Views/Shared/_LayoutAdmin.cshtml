@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en" class="js no-touch sb-init">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <base href="http://localhost:62388/" />
    <title>Shopee Template</title>
    <!-- Bootstrap -->
    <link href="Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="Scripts/bootstrap.min.js" type="text/javascript"></script>
    <link href="Content/bootstrap-reset.css" rel="stylesheet" type="text/css" />
    <link href="Content/style.css" rel="stylesheet" type="text/css" />
    <section id="main-content">
          <section class="wrapper">
           @RenderBody()
           </section>
           </section>
    <script src="Scripts/alertify.min.js" type="text/javascript"></script>
    <link href="Content/alertifyjs/alertify.min.css" rel="stylesheet" type="text/css" />
    <link href="Content/alertifyjs/themes/default.min.css" rel="stylesheet" type="text/css" />
    <script src="Scripts/custom.js" type="text/javascript"></script>
    <script type="text/javascript">  @RenderSection("pagescripts", required: false)</script>
    <script type="text/javascript">
        function ShowConfirmDelete(deleteAPIUrl) {
            var title = "Delete";
            var message = "Are you sure?";
            var onok = function () {
                CallDelete(deleteAPIUrl);
            };
            var oncancel = function () {
                return true;
            };
            alertify.confirm(title, message, onok, oncancel);
        }

        function CallDelete(url) {
            
            $.post(url, function (data) {
                debugger; if (data.Code == 200) {
                    alertify.success('Successfully Deleted!');
                    setInterval(function () {
                        window.location.reload();
                    }, 2000);
                }
            }).fail(function () {
                alertify.error('error');
            });
        }
        $(function () {
            $('[data-toggle="tooltip"]').tooltip();
         
        });
    </script>
</body>
</html>
